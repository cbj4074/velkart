{
  "name": "indiehd/velkart",
  "description": "An extensible shopping cart, order management, and payment processing system built upon clean-coding principles and designed for Laravel.",
  "license": "GNU AFFERO GENERAL PUBLIC LICENSE Version 3",
  "authors": [
    {
      "name": "Everett M Wiley (Poppabear)",
      "email": "servnx@gmail.com"
    },
    {
      "name": "Ben Johnson (cbj4074)",
      "email": "ben@indiehd.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "bumbummen99/shoppingcart": "^2.0",
    "baum/baum": "v2.x-dev",
    "illuminate/support": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "IndieHD\\Velkart\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "IndieHD\\Velkart\\Database\\Seeds\\": "database/seeds/",
      "IndieHD\\Velkart\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "fzaninotto/faker": "^1.9.1",
    "orchestra/testbench": "^5.0",
    "mockery/mockery": "^1.3.1",
    "php-coveralls/php-coveralls": "^2.1",
    "phpro/grumphp": "^0.18.1"
  },
  "extra": {
    "laravel": {
      "providers": [
        "IndieHD\\Velkart\\VelkartServiceProvider"
      ]
    }
  }
}
